<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: black;" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.view.SearchUserController">
   <children>
      <Button fx:id="USBackbt" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#Backbt" text="BACK">
         <font>
            <Font name="VCR OSD Mono" size="12.0" />
         </font></Button>
      <Label layoutX="222.0" layoutY="40.0" text="SEARCH USER" textFill="WHITE">
         <font>
            <Font name="VCR OSD Mono" size="24.0" />
         </font>
      </Label>
      <TextField fx:id="USIDSearch" layoutX="132.0" layoutY="160.0" promptText="ID" />
      <TextField fx:id="USNameSearch" layoutX="393.0" layoutY="160.0" promptText="Name" />
      <Button fx:id="USSIDbt" layoutX="60.0" layoutY="160.0" mnemonicParsing="false" onAction="#SearchIDbt" text="Search">
         <font>
            <Font name="VCR OSD Mono" size="12.0" />
         </font></Button>
      <Button fx:id="NameSbt" layoutX="325.0" layoutY="160.0" mnemonicParsing="false" onAction="#SearchNamebt" text="Search">
         <font>
            <Font name="VCR OSD Mono" size="12.0" />
         </font></Button>
   </children>
</AnchorPane>
