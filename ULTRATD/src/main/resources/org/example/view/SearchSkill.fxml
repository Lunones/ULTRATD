<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: black;" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.view.SearchSkillController">
   <children>
      <Button fx:id="SKBackbt" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#Backbt" text="BACK">
         <font>
            <Font name="VCR OSD Mono" size="12.0" />
         </font></Button>
      <TextField fx:id="SKIDSearch" layoutX="112.0" layoutY="170.0" promptText="ID" />
      <TextField fx:id="SKNameSearch" layoutX="397.0" layoutY="170.0" promptText="Name" />
      <Button fx:id="SKSIDbt" layoutX="37.0" layoutY="170.0" mnemonicParsing="false" onAction="#SearchIDbt" text="Search">
         <font>
            <Font name="VCR OSD Mono" size="12.0" />
         </font></Button>
      <Button fx:id="SKNameSbt" layoutX="325.0" layoutY="170.0" mnemonicParsing="false" onAction="#SearchNamebt" text="Search">
         <font>
            <Font name="VCR OSD Mono" size="12.0" />
         </font></Button>
      <Label layoutX="216.0" layoutY="52.0" text="SEARCH SKILL" textFill="WHITE">
         <font>
            <Font name="VCR OSD Mono" size="24.0" />
         </font>
      </Label>
      <Button fx:id="FAS" layoutX="256.0" layoutY="260.0" mnemonicParsing="false" onAction="#FindAS" text="Search All">
         <font>
            <Font name="VCR OSD Mono" size="12.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
